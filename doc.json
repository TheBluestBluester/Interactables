{
  "name": "Interactables",
  "description": "Make your stuff interactable!",
  "author": "Bluester16",
  "config": {
    "TrustedPlayers": {
      "description": "Players that you trust to place down codeblocks.",
      "default": [],
      "type": "players"
    },
    "TrustedRole": {
      "description": "Role that you trust to place down codeblocks.",
      "default": [],
      "type": "role"
    },
    "UpdateFrequency": {
      "description": "How fast the plugin checks for zones. Frequency is in seconds.",
      "default": 1,
      "type": "number"
    },
    "DisableZones": {
      "description": "Disables zones.",
      "default": false,
      "type": "boolean"
    }
  },
  "commands": [
    {
      "name": "/use",
      "description": "Use it near a trigger to activate functions.",
      "example": "use",
      "args": []
    },
    {
      "name": "/place",
      "description": "Place codeblocks at the location of your ghost brick.",
      "example": "place message Hello world!",
      "args": [
        {
          "name": "Codeblock name",
          "description": "Name of a codeblock you want to place.",
          "required": true
        },
        {
          "name": "args...",
          "description": "Args. Amount and type depends on the codeblock.",
          "required": false
        }
      ]
    },
    {
      "name": "/clearblacklist",
      "description": "Clear blacklist.",
      "example": "clearblacklist",
      "args": []
    },
    {
      "name": "/clearvariables",
      "description": "Clear all variables.",
      "example": "clearvariables",
      "args": []
    },
    {
      "name": "/listblacklisted",
      "description": "Lists all blacklisted functions.",
      "example": "listblacklisted",
      "args": []
    },
    {
      "name": "/listvariables",
      "description": "Lists all variables.",
      "example": "listvariables",
      "args": []
    },
    {
      "name": "/stopall",
      "description": "Stops all functions.",
      "example": "stopall",
      "args": []
    },
    {
      "name": "/door",
      "description": "Places selected bricks with the selector as a door.",
      "example": "door",
      "args": [
        {
          "name": "door name",
          "description": "Door's name.",
          "required": true
        }
      ]
    }
  ]
}
